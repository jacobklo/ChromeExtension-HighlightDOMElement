{
	"manifest_version" : 2,

	"name" : "HighlightDOMElement"
	,"description" : " - Highlight a DOM element inside a webpage wherever the mouse hover to. Return that HTML tag.  Highlight code is from Adblock, credit Adblock's developers."
	,"version" : "0.1"
 
  ,"background" : {
    "page" : "background.html"
  }
  ,"permissions": ["alarms", "notifications", "*://*/*" ,"tabs", "<all_urls>"]
}